{
    "name": "rocketphp/template",
    "description": "Dynamic HTML templates.",
    "type": "library",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Eric Mugerwa",
            "email":  "eric@rocketphp.io",
            "homepage":  "http://www.rocketphp.io",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "keywords": [
        "rocketphp",
        "template",
        "html"
    ],
    "homepage": "https://github.com/rocketphp/template",
    "autoload": {
        "psr-4": {
            "RocketPHP\\Template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RocketPHPTest\\Template\\": "test/"
        }
    },
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "suggest": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    }
}